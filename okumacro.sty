\def\whichpage#1{%
  \expandafter\ifx\csname r@#1\endcsname\relax
  \else
    \@tempcnta\pageref{#1}\relax
    \advance\@tempcnta-\c@page
    \ifnum\@tempcnta=0
    \else
      \ifnum\@tempcnta=1 （次ページ）
      \else
        \ifnum\@tempcnta=-1 （前ページ）
        \else （\pageref{#1}ページ）
        \fi
      \fi
    \fi
  \fi}
